{
  "name": "@staart/manager",
  "version": "1.3.12",
  "main": "index.js",
  "repository": "git@github.com:staart/api.git",
  "author": "Anand Chowdhary <mail@anandchowdhary.com>",
  "license": "MIT",
  "scripts": {
    "test": "jest --forceExit",
    "build": "touch .env && mkdir -p dist && cp .env dist/.env && npm run generate-routes && tsc",
    "build-babel": "touch .env && babel src -d dist --extensions '.ts' && cp .env dist/.env",
    "start": "npm run build && npm run launch",
    "launch": "node dist/src/index.js",
    "dev": "concurrently 'npm run watch-build' 'nodemon dist/src/index.js'",
    "prettier-all": "prettier '{src,examples,setup}/**/*.{ts,js,json}' --write",
    "watch-build": "onchange '{src,examples}/**/*.ts' -- npm run build-babel",
    "watch-prettier": "onchange '{src,examples}/**/*.{ts,json}' -- prettier --write {{changed}}",
    "generate-docs": "npm run generate-routes && typedoc --out docs ./src",
    "generate-routes": "node setup/controllers.js",
    "increment-package": "node setup/increment.js",
    "check-update": "node setup/check-update.js",
    "update": "git pull git@github.com:staart/api",
    "postinstall": "npm run check-update",
    "publish-package": "npm publish --access public",
    "snyk-protect": "snyk protect"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run increment-package && npm run prettier-all && git add ."
    }
  },
  "engines": {
    "node": ">= 10.16.0"
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    }
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.6",
    "@babel/plugin-proposal-decorators": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/polyfill": "^7.8.3",
    "@babel/preset-env": "^7.8.6",
    "@babel/preset-typescript": "^7.8.3",
    "@overnightjs/core": "^1.6.14",
    "@types/bcryptjs": "^2.4.2",
    "@types/cron": "^1.7.2",
    "@types/express": "^4.17.2",
    "@types/express-slow-down": "^1.3.0",
    "@types/fs-extra": "^8.1.0",
    "@types/geolite2": "^2.0.0",
    "@types/hapi__joi": "^16.0.12",
    "@types/jest": "^25.1.3",
    "@types/jsonwebtoken": "^8.3.7",
    "@types/ms": "^0.7.31",
    "@types/mysql": "^2.15.9",
    "@types/node": "^13.7.6",
    "@types/node-emoji": "^1.8.1",
    "@types/qrcode": "^1.3.4",
    "@types/randomcolor": "^0.5.3",
    "@types/request": "^2.48.4",
    "@types/signale": "^1.2.1",
    "@types/validator": "^12.0.1",
    "@types/yaml": "^1.2.0",
    "concurrently": "^5.1.0",
    "coveralls": "^3.0.9",
    "dotenv": "^8.2.0",
    "has-yarn": "^2.1.0",
    "husky": "^4.2.3",
    "jest": "^25.1.0",
    "onchange": "^6.1.0",
    "prettier": "^1.19.1",
    "recursive-readdir": "^2.2.2",
    "shelljs": "^0.8.3",
    "ts-jest": "^25.2.1",
    "typedoc": "^0.16.10",
    "typescript": "^3.8.2",
    "yaml": "^1.7.2"
  },
  "dependencies": {
    "@sentry/node": "^5.12.5",
    "@staart/disposable-email": "^2.0.0",
    "@staart/elasticsearch": "^2.0.0",
    "@staart/errors": "^1.2.1",
    "@staart/mail": "^2.0.0",
    "@staart/messages": "^2.0.0",
    "@staart/mustache-markdown": "^3.0.0",
    "@staart/payments": "^2.0.0",
    "@staart/redis": "^2.0.0",
    "@staart/server": "^1.0.0",
    "@staart/text": "^2.0.0",
    "@staart/validate": "^2.1.0",
    "axios": "^0.19.2",
    "bcryptjs": "^2.4.3",
    "client-oauth2": "^4.2.5",
    "cron": "^1.8.2",
    "express": "^4.17.1",
    "express-async-handler": "^1.1.4",
    "express-rate-limit": "^5.1.1",
    "express-slow-down": "^1.3.1",
    "fs-extra": "^8.1.0",
    "geolite2-redist": "^1.0.4",
    "ip-range-check": "^0.2.0",
    "jsonwebtoken": "^8.5.1",
    "matcher": "^2.1.0",
    "maxmind": "^4.1.0",
    "ms": "^2.1.2",
    "mysql": "^2.18.1",
    "node-cache": "^5.1.0",
    "node-emoji": "^1.10.0",
    "otplib": "^12.0.1",
    "qrcode": "^1.4.4",
    "random-int": "^2.0.1",
    "randomcolor": "^0.5.4",
    "snyk": "^1.297.2",
    "systeminformation": "^4.22.3"
  },
  "files": [
    "setup"
  ],
  "snyk": true,
  "staart-version": "1.3.12"
}