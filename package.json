{
  "name": "staart",
  "version": "1.0.31",
  "main": "index.js",
  "repository": "git@github.com:AnandChowdhary/staart.git",
  "author": "Anand Chowdhary <mail@anandchowdhary.com>",
  "license": "MIT",
  "scripts": {
    "build-babel": "touch .env && babel src -d dist --extensions '.ts' && cp .env dist/.env",
    "build": "touch .env && mkdir -p dist && cp .env dist/.env && npm run generate-routes && tsc",
    "start": "npm run build && node dist/index.js",
    "dev": "concurrently 'yarn watch-build' 'yarn watch-server'",
    "prettier-all": "prettier '{src,examples,setup}/**/*.{ts,js,json}' --write",
    "watch-build": "onchange '{src,examples}/**/*.ts' -- yarn build-babel",
    "watch-prettier": "onchange '{src,examples}/**/*.{ts,json}' -- prettier --write {{changed}}",
    "watch-server": "cd dist && nodemon index.js",
    "generate-docs": "yarn generate-routes && typedoc --out docs ./src",
    "generate-routes": "node setup/controllers.js",
    "increment-package": "versiony --patch",
    "update": "git pull git@github.com:o15y/staart"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn increment-package && yarn prettier-all && git add ."
    }
  },
  "engines": {
    "node": "10.16.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-typescript": "^7.3.3",
    "@overnightjs/core": "^1.6.9",
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.5",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.0",
    "@types/express-brute": "^0.0.37",
    "@types/express-slow-down": "^1.1.0",
    "@types/fs-extra": "^7.0.0",
    "@types/geolite2": "^1.2.0",
    "@types/hapi__joi": "^15.0.2",
    "@types/helmet": "^0.0.43",
    "@types/jest": "^24.0.15",
    "@types/jsonwebtoken": "^8.3.2",
    "@types/marked": "^0.6.5",
    "@types/md5": "^2.1.33",
    "@types/morgan": "^1.7.35",
    "@types/mustache": "^0.8.32",
    "@types/mysql": "^2.15.6",
