{
  "name": "staart",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@github.com:AnandChowdhary/staart.git",
  "author": "Anand Chowdhary <mail@anandchowdhary.com>",
  "license": "MIT",
  "scripts": {
    "build-babel": "babel src -d dist --extensions '.ts' && cp .env dist/.env",
    "build": "touch .env && mkdir -p dist && cp .env dist/.env && tsc",
    "check-types": "tsc",
    "start": "pm2-runtime start dist/index.js --env production",
    "dev": "concurrently 'yarn watch-build' 'yarn watch-server'",
    "prettier-all": "prettier '{src,examples}/**/*.{ts,json}' --write",
    "watch-build": "onchange '{src,examples}/**/*.ts' -- yarn build-babel",
    "watch-prettier": "onchange '{src,examples}/**/*.{ts,json}' -- prettier --write {{changed}}",
    "watch-server": "cd dist && nodemon index.js",
    "generate-docs": "typedoc --out docs ./src"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn prettier-all && git add . && yarn check-types"
    }
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.4",
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-typescript": "^7.3.3",
    "@overnightjs/core": "^1.6.3",
    "@types/bcrypt": "^3.0.0",
    "@types/cors": "^2.8.5",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.16.1",
    "@types/fs-extra": "^7.0.0",
    "@types/geolite2": "^1.2.0",
    "@types/helmet": "^0.0.43",
    "@types/jest": "^24.0.13",
    "@types/jsonwebtoken": "^8.3.2",
    "@types/marked": "^0.6.5",
    "@types/md5": "^2.1.33",
    "@types/morgan": "^1.7.35",
    "@types/mustache": "^0.8.32",
    "@types/mysql": "^2.15.6",
    "@types/node": "^12.0.2",
    "@types/request": "^2.48.1",
    "@types/response-time": "^2.3.3",
    "@types/stripe": "^6.26.0",
    "@types/validator": "^10.11.0",
    "concurrently": "^4.1.0",
    "coveralls": "^3.0.3",
    "dotenv": "^8.0.0",
    "husky": "^2.3.0",
    "jest": "^24.8.0",
    "nodemon": "^1.19.0",
    "onchange": "^6.0.0",
    "pm2": "^3.5.1",
    "prettier": "^1.17.1",
    "ts-jest": "^24.0.2",
    "typedoc": "^0.14.2",
    "typescript": "^3.4.5"
  },
  "dependencies": {
    "bcrypt": "^3.0.6",
    "body-parser": "^1.19.0",
    "chargebee": ">=2 <3",
    "cors": "^2.8.5",
    "crypto-random-string": "^3.0.0",
    "express": "^4.17.0",
    "express-async-handler": "^1.1.4",
    "fs-extra": "^8.0.1",
    "geolite2": "^1.2.1",
    "googleapis": "^40.0.0",
    "helmet": "^3.18.0",
    "ip-anonymize": "^0.0.6",
    "jsonwebtoken": "^8.5.1",
    "marked": "^0.6.2",
    "maxmind": "^3.0.4",
    "md5": "^2.2.1",
    "morgan": "^1.9.1",
    "mustache": "^3.0.1",
    "mysql": "^2.17.1",
    "node-cache": "^4.2.0",
    "node-emoji": "^1.10.0",
    "node-ses": "^2.2.1",
    "response-time": "^2.3.2",
    "rotating-file-stream": "^1.4.1",
    "stripe": "^7.1.0",
    "validator": "^11.0.0"
  }
}
